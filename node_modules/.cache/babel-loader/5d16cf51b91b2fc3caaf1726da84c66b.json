AAA;AACA;AAPA;AA7BA,CAAA","sourcesContent":["<template>\n  <body>\n  <div class=\"app-containerproduct-box__btn pt-40\">\n\n\n    <div class=\"wrap\">\n      <section class=\"container\">\n        <div class=\"filter-box\">\n          <div class=\"select-box\">\n            <label class=\"filter-label\">Категория</label>\n            <select class=\"select-control\" id=\"food-filter\">\n              <option value=\"0\">Все</option>\n              <option value=\"1\">Завтраки</option>\n              <option  value=\"2\">Первые блюда</option>\n              <option  value=\"3\">Гарниры</option>\n            </select>\n          </div><!-- /.select-box -->\n          <div class=\"price-select-box\">\n            <label class=\"filter-label\">Цена</label>\n            <select class=\"select-control\" id=\"price-filter\">\n              <option value=\"0\">Все</option>\n              <option value=\"30\">До 30 грн</option>\n              <option value=\"50\">До 50 грн</option>\n              <option value=\"100\">До 100 грн</option>\n              <option value=\"150\">До 150 грн</option>\n            </select>\n            <RouterLink to=\"/cart\">Cart</RouterLink>\n          </div><!-- /.price-select-box -->\n          <div class=\"cart\" style=\"float: right\">\n            {{CART.length}}\n          </div>\n        </div><!-- /filter-box-->\n\n\n\n        <div class=\"products-box grid-box\" >\n\n          <div class=\"product-box__item\"\n               :data-id=\"product.id\"\n               v-for=\"(product) in allProducts\"\n               v-bind:key=\"product.id\">\n\n            <h3 class=\"product-box__title\">{{product.title}}</h3>\n            <div class=\"product-box__img\">\n              <img class=\"img-fluid\" :src=product.image>\n            </div>\n            <div class=\"product-box__meta\">\n              <p>{{product.price}}</p>\n              <div class=\"qty\">\n                <button @click=\"increment(product.id)\">+</button>\n                <button @click=\"decrement(product.id)\">-</button>\n                <span>{{getTotalQua}}</span>\n              </div>\n              <button class=\"product-box__btn\"\n                      :data-id=\"product.id\"\n                      :data-price=\"product.price\"\n                      :data-qua=\"product.quantity\"\n                      @click=\"addToCart\"\n                      >\n                Добавить\n              </button>\n\n            </div>\n          </div>\n\n        </div><!-- /product-box-->\n\n      </section>\n    </div>\n\n  </div><!-- /.app-container -->\n  </body>\n\n</template>\n\n<script>\nimport {mapGetters} from 'vuex'\nimport {mapActions} from 'vuex'\nexport default {\n  name: \"Product\",\n  data(){\n    return{\n      productData:[],\n      currentQua: 0\n    }\n  },\n  methods:{\n    ...mapGetters(['getAllProducts']),\n    ...mapActions(['ADD_TO_CART','INCREMENT','DECREMENT']),\n\n    addToCart(e){\n      let product = {\n        id:e.currentTarget.dataset.id,\n        price:e.currentTarget.dataset.price,\n        quantity:e.currentTarget.dataset.qua\n      }\n      this.ADD_TO_CART(product)\n      this.currentQua = product.quantity\n    },\n    increment(id){\n      this.INCREMENT(id)\n    },\n    decrement(id){\n      this.DECREMENT(id)\n    }\n\n  },\n  computed:{\n    ...mapGetters(['CART']),\n    allProducts(){\n      return this.$store.getters.getAllProducts\n    },\n    getTotalQua(){\n      return this.$store.getters.QUA\n    }\n  },\n\n\n\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/views"}]}